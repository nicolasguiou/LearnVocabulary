<?xml version="1.0" encoding="utf-8"?>
<s:TabbedViewNavigatorApplication 
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark"
	needsSoftKeyboard="true">
	
	<fx:Style source="StyleComponents.css" /> 
	
	<fx:Metadata>
		[ResourceBundle("myResources")]
	</fx:Metadata>
	
	<s:ViewNavigator id="homeViewNavigator" 	label="{resourceManager.getString('myResources','HOME.Home')}" 			width="100%" height="100%" firstView="views.HomeView" />
	<s:ViewNavigator id="dataBaseViewNavigator" label="{resourceManager.getString('myResources','DATABASE.DataBase')}" 	width="100%" height="100%" firstView="views.VocabularyView"/>
	<s:ViewNavigator id="exerciseViewNavigator" label="{resourceManager.getString('myResources','EXERCISE.Exercise')}" 	width="100%" height="100%" firstView="views.ExerciceView"/>
	
</s:TabbedViewNavigatorApplication>